.panel-heading
  %h1 
    %i.icon-copy
    = @project.title
    Duplications
.list-group
  .list-group-item
    %h4 File churns
  .list-group-item
    #file-churn-icecle
  .list-group-item
    %table.table.table-striped.churn
      %th.name File path
      %th.times Times changed
      - @report.scores.file_churns.each do |churn|
        %tr
          %td= link_to churn.targetable.path, project_report_file_path(@project, @report, file_id: churn.targetable)
          %td= churn.score

